@model aereoliniafinal.Models.TipoSexo

@{
    ViewData["Title"] = "Create";
}

<div class="card">
    <h3 class="card-header font-weight-bold text-uppercase py-4  text-center">
        Ingrese el sexo
    </h3>

</div>
<br />

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Sexo" class="control-label"></label>
                <input asp-for="Sexo" class="form-control" />
                <span asp-validation-for="Sexo" class="text-danger"></span>
            </div>
            <a type="button" class="btn btn-success" asp-action="Index">Volver atras </a> |
            <input type="submit" value="Crear" class="btn btn-primary" />

        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
